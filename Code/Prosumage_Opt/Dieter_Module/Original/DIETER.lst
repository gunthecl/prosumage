      $ontext
              + sum( (bu,ch) , theta_dir(n,bu,ch) * lev_H_DIR(scen,n,bu,ch,h) + 
      theta_sets(n,bu,ch) * lev_H_SETS_IN(scen,n,bu,ch,h) + theta_hp(n,bu,ch) * 
