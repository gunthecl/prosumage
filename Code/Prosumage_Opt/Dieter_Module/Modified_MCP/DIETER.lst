1260  con11a_pro_distrib(res,h)..
1261           phi_res(res,h) * N_RES_PRO(res)
1262           =E=
1263           CU_PRO(res,h) + G_MARKET_PRO2M(res,h) + G_RES_PRO(res,h) + sum( sto , STO_IN_PRO2PRO(res,sto,h) )
1264  ;
1265   
1266  con11b_pro_balance(h)..
1267           phi_pro_load * d(h)
1268           =E=
1269           sum( res , G_RES_PRO(res,h)) + sum( sto , STO_OUT_PRO2PRO(sto,h) ) + G_MARKET_M2PRO(h)
1270  ;
1271   
1272  con11c_pro_selfcon..
1273           sum( (h,res) , G_RES_PRO(res,h) ) + sum( (h,sto) , STO_OUT_PRO2PRO(sto,h) )
1274           =G=
1275           phi_pro_self * sum( h , phi_pro_load * d(h))
1276  ;
1277   
1278  con11d_pro_stolev_PRO2PRO(sto,h)$(ord(h) > 1 )..
1279           STO_L_PRO2PRO(sto,h) =E= STO_L_PRO2PRO(sto,h-1) + sum( res , STO_IN_PRO2PRO(res,sto,h))*(1+eta_sto(sto))/2 - STO_OUT_PRO2PRO(sto,h)/(1+eta_sto(sto))*2
1280  ;
1281   
1282   
1283  con11h_1_pro_stolev_start_PRO2PRO(sto,h)$( ord(h) = 1)..
