1138  ***** Hourly maximum generation caps *****
1139  * ---------------------------------------------------------------------------- *
1140   
1141  con3a_maxprod_dispatchable(dis,h)..
1142   
1143           N_TECH(dis) -  G_L(dis,h)  =G= 0
1144   
1145  ;
1146   
1147  con3e_maxprod_res(nondis,h)..
1148   
1149   
1150         phi_res(nondis,h)*N_TECH(nondis) - G_RES(nondis,h) - CU(nondis,h) =E= 0
1151  ;
1152   
1153  * ---------------------------------------------------------------------------- *
1154  ***** Storage constraints *****
1155  * ---------------------------------------------------------------------------- *
1156   
1157  con4a_stolev_start(sto,h)$(ord(h) = 1)..
1158  *        phi_sto_ini(sto) * N_STO_E(sto)
1159          + STO_IN(sto,h)*(1+eta_sto(sto))/2 - STO_OUT(sto,h)/(1+eta_sto(sto))*2  - STO_L(sto,h)  =E= 0
1160  ;
1161   
1162  con4b_stolev(sto,h)$(ord(h)>1)..
1163          STO_L(sto,h-1) + STO_IN(sto,h)*(1+eta_sto(sto))/2 - STO_OUT(sto,h)/(1+eta_sto(sto))*2  -  STO_L(sto,h) =E= 0
1164  ;
1165   
1166  con4_stolev(sto,h)..
1167   
1168         + STO_IN(sto,h)*(1+eta_sto(sto))/2 - STO_OUT(sto,h)/(1+eta_sto(sto))*2  -  STO_L(sto,h)
1169  *      + (phi_sto_ini(sto)*N_STO_E(sto))$(ord(h) = 1)
1170         + (STO_L(sto,h-1))$(ord(h)>1)
1171         =E= 0
1172  ;
1173   
1174   
1175  con4c_stolev_max(sto,h)..
1176         N_STO_E(sto) -  STO_L(sto,h) =G= 0
1177  ;
1178   
1179  con4d_maxin_sto(sto,h)..
1180   
1181          N_STO_P(sto) -  STO_IN(sto,h)   =G= 0
1182  ;
1183   
1184  con4e_maxout_sto(sto,h)..
1185   
1186          N_STO_P(sto) - STO_OUT(sto,h)   =G= 0
1187  ;
1188   
1189  con4j_ending(sto,h)$(ord(h) = card(h))..
1190   
1191          phi_sto_ini(sto) * N_STO_E(sto) -  STO_L(sto,h) =E= 0
1192  ;
1193   
1194   
1195  con4k_PHS_EtoP(sto)..
1196           etop_max(sto) * N_STO_P(sto) - N_STO_E(sto) =G= 0
1197  ;
1198   
1199  * ---------------------------------------------------------------------------- *
1200  ***** Quotas for renewables and biomass *****
1201  * ---------------------------------------------------------------------------- *
1202   
1203  con5a_minRES..
1204  sum( h , G_L('bio',h) + sum(nondis , G_RES(nondis,h))
1205   
               + sum( (sto) , STO_OUT_PRO2PRO(sto,h) + sum( (res) , G_MARKET_PRO2M(res,h) + G_RES_PRO(res,h))
      $ontext
1210  )
1211          - phi_min_res * phi_min_res_exog * sum( h ,
1212           sum( (dis) , G_L(dis,h)) + sum( (nondis) , G_RES(nondis,h))
1213   
               + sum( (res) , phi_res(res,h) * N_RES_PRO(res) - CU_PRO(res,h))
      $ontext
1218           )  =G= 0
