 501  stolev_max(h)..
 502   
 503            N_STO_E  =G= STO_L( h)
 504  ;
 505   
 506   
 507  maxin_power(h)..
 508   
 509           N_STO_P  =G=  STO_IN( h)
 510  ;
 511   
 512  maxout_power(h)..
 513   
 514         N_STO_P   =G=  STO_OUT( h)
 515  ;
 516   
 517   
 518  KKTNRES..
 519   
 520   
 521    c_i_res - sum( h, mu(h)*phi_res(h)) =G= 0
